<form method="get" action="/quizes">
	<input type="text" name="search" placeholder="Buscar" />
	<button type="submit">Buscar</button>
</form>
<table>
<%  var i = 0;
    for( i = 0; i < quizes .length; i++ ) { 	%>
    <tr>
    	<td>
    		<a href="quizes/<%= quizes[ i ] .id %>">
    			<%= quizes[ i ] .pregunta %>
    		</a>
    	</td>
    </tr>

<%  }  %>
</table>